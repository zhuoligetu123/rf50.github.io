          "title": "Bayes Filtering",
          "semantic_type": "Concept",
          "is_public": true,
          "user_state": {
            "node_key": "e3a9d449-b4de-40f3-91f7-87bb81525541",
            "completed_at": null,
            "last_viewed_at": null,
